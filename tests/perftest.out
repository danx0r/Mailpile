def histogram(r):
  buckets = {
    0.025: 0,
    0.050: 0,
    0.100: 0,
    0.250: 0,
    0.500: 0,
    1.000: 0
  }
  for t, x in r:
    for b in buckets:
      if t <= b: buckets[b] += 1
  for b in sorted(buckets.keys()): 
    pct = 100*buckets[b]/len(r)
    print ('  %1.3f  %2.2d  %s  %s   - %s%%'
           ) % (b, buckets[b],
                pct > 50 and '50%' or '   ',
                pct  > 75 and '75%' or '   ', pct)

for r in sorted(results.keys()):
  print '%s' % r
  histogram(results[r])

